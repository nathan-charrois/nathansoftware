{
  "name": "nathan-software",
  "type": "module",
  "scripts": {
    "build": "npm run app:build && npm run server:build",
    "start": "npm --prefix server run dev",
    "app:dev": "react-router dev",
    "app:build": "react-router build",
    "server:dev": "cd server && npm run dev",
    "server:build": "tsc --project server",
    "app:start": "react-router-serve ./build/server/index.js",
    "app:typecheck": "react-router typegen && tsc",
    "app:lint": "eslint . --fix"
  },
  "dependencies": {
