    "@mantine/form": "^8.0.1",
    "@mantine/hooks": "^7.17.5",
    "@react-router/node": "^7.5.2",
    "@react-router/serve": "^7.5.2",
    "isbot": "^5.1.17",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.8.2"
  },
  "devDependencies": {
    "@react-router/dev": "^7.8.2",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "eslint": "^9.25.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "react-router-devtools": "^1.1.0",
    "tailwindcss": "^4.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2",
